<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">
    <transfer name="TourGuide">
        <property name="idTourGuide" type="int"/>
        <property name="fkAclGroup" type="int"/>
        <property name="aclGroup" type="Group"/>
        <property name="route" type="string"/>
        <property name="version" type="int"/>
        <property name="isActive" type="bool"/>
        <property name="tourGuideSteps" type="TourGuideStep[]" singular="tourGuideStep"/>
        <property name="tourGuideEvents" type="TourGuideEvent[]" singular="tourGuideEvent"/>
    </transfer>

    <transfer name="TourGuideCollection">
        <property name="tourGuides" type="TourGuide[]" singular="tourGuide"/>
    </transfer>

    <transfer name="TourGuideCriteria">
        <property name="idTourGuide" type="int"/>
        <property name="fkAclGroup" type="int"/>
        <property name="route" type="string"/>
        <property name="isActive" type="bool"/>
    </transfer>

    <transfer name="TourGuideStep">
        <property name="idTourGuideStep" type="int"/>
        <property name="fkTourGuide" type="int"/>
        <property name="tourGuide" type="TourGuide"/>
        <property name="stepIndex" type="int"/>
        <property name="title" type="string"/>
        <property name="text" type="string"/>
        <property name="attachToElement" type="string"/>
        <property name="attachToPosition" type="string"/>
        <property name="isActive" type="bool"/>
    </transfer>

    <transfer name="TourGuideStepCollection">
        <property name="tourGuideSteps" type="TourGuideStep[]" singular="tourGuideStep"/>
    </transfer>

    <transfer name="TourGuideStepCriteria">
        <property name="idTourGuideStep" type="int"/>
        <property name="fkTourGuide" type="int"/>
        <property name="isActive" type="bool"/>
    </transfer>

    <transfer name="TourGuideEvent">
        <property name="idTourGuideEvent" type="int"/>
        <property name="fkTourGuide" type="int"/>
        <property name="tourGuide" type="TourGuide"/>
        <property name="eventType" type="string"/>
        <property name="tourVersion" type="int"/>
        <property name="createdAt" type="string"/>
    </transfer>

    <transfer name="TourGuideEventCollection">
        <property name="tourGuideEvents" type="TourGuideEvent[]" singular="tourGuideEvent"/>
    </transfer>

    <transfer name="TourGuideEventCriteria">
        <property name="idTourGuideEvent" type="int"/>
        <property name="fkTourGuide" type="int"/>
        <property name="eventType" type="string"/>
        <property name="route" type="string"/>
    </transfer>

    <transfer name="TourGuideButton">
        <property name="text" type="string"/>
        <property name="action" type="string"/>
        <property name="classes" type="string"/>
    </transfer>

    <transfer name="RouteValidationRequest">
        <property name="route" type="string"/>
        <property name="validRoutes" type="string[]" singular="validRoute"/>
    </transfer>

    <transfer name="Group">
        <property name="idAclGroup" type="int"/>
        <property name="name" type="string"/>
    </transfer>
</transfers>
